{
  "name": "vuedcast",
  "version": "1.0.0",
  "description": "A Vue podcast player",
  "repository": {
    "type": "git",
    "url": "https://github.com/Myugen/vuedcast.git"
  },
  "main": "index.html",
  "scripts": {
    "dev": "npx parcel serve src/index.html --open",
    "watch": "npx parcel watch src/index.html --global global",
    "build": "rm -rf build/* && npx parcel build src/index.html -d build --global global --public-url /",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "git subtree push --prefix build origin gh-pages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "parcel",
    "podcast",
    "html5",
    "player",
    "css3",
    "postcss",
    "stylelint",
    "javascript",
    "eslint",
    "vue"
  ],
  "author": "Myugen",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@vue/component-compiler-utils": "^2.6.0",
    "autoprefixer": "^9.4.10",
    "husky": "1.3.1",
    "npm-run-all": "^4.1.3",
    "parcel": "1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss-clean": "^1.1.0",
    "postcss-font-magician": "^2.2.1",
    "postcss-mixins": "^6.2.1",
    "postcss-preset-env": "^6.6.0",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.2",
    "typescript-tslint-plugin": "^0.3.1",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.8"
  },
  "dependencies": {
    "@ibm/plex": "^1.3.1",
    "vue": "^2.6.8",
    "vue-class-component": "^7.0.2"
  }
}
